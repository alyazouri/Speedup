  // ÙØ­Øµ ÙƒÙ„ Pattern
  for (var key in PATTERNS) {
    var pattern = PATTERNS[key];
    
    // ÙØ­Øµ Ø§Ù„ÙƒÙ„Ù…Ø§Øª Ø§Ù„Ù…ÙØªØ§Ø­ÙŠØ© ÙÙŠ Host
    for (var i = 0; i < pattern.keywords.length; i++) {
