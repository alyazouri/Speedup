    }
    
    // ÙØ­Øµ Ø§Ù„Ù…Ø³Ø§Ø±Ø§Øª ÙÙŠ URL
    for (var j = 0; j < pattern.paths.length; j++) {
      if (urlLower.indexOf(pattern.paths[j]) !== -1) {
