    }
    
    // ÙØ­Øµ Ports (Ø¥Ø°Ø§ Ù…ÙˆØ¬ÙˆØ¯Ø©)
    if (pattern.ports) {
      for (var k = 0; k < pattern.ports.length; k++) {
        if (urlLower.indexOf(pattern.ports[k]) !== -1) {
          return pattern.type;
        }
      }
    }
  }
  
  return null;
}

